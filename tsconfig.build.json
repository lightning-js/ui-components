{
  "exclude": ["node_modules/**/*"],
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "ES5"],
    "strict": true,
    "target": "ESNext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "composite": true,
    "types": ["vite/client"],
    "noEmit": false,
    "isolatedModules": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  }
}
